/*public class Node{
	public static void main(String[] args) {
		
		System.out.println("Hello World");	}
}*/

class Node
{
	int data;
	Node next;
	Node(int data){
		this.data = data;
	}
	
	
    public static void main(String[] args)
    {
        System.out.println("My Linked List program.");
     //Linked List flow
     // 6->8->10->12->5
        Node nodeA=new Node(6);
        Node nodeB=new Node(8);
        Node nodeC=new Node(10);
        Node nodeD=new Node(12);
        Node nodeE=new Node(5);
        nodeA.next=nodeB;
        nodeB.next=nodeC;
        nodeC.next=nodeD;
        nodeD.next=nodeE;
        
    }
}